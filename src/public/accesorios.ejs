<body>
    <body>
        <header class="red">
            <img src="img/logo.png" alt="">
            <a class="menu_toggle">â˜°</a>
            
              
                
                
                <ul>
                    <li><a href="/">INICIO</a></li>
                    <li><a href="/producto">PRODUCTOS</a></li>
                    <li><a href="/descuentos">PROMOCIONES</a></li>
                    <li><a href="/nosotros">NOSOTROS</a></li>
                    
                    <li><a href="search" class="bi bi-search"></a></li>
                    <li><a href="cart" class="bi bi-cart"></a>
                      <!--<a class="objeto" href="cart"><%= objeto%></a>-->
                    </li>
                    
                    <li>
                        <% if ( usuario ){ %>
                            <a href=""><%= usuario %></a>
                            <li><a href="/logout" class="bi bi-box-arrow-right"></a></li>
                        <% }else{%>
                            <a href="login" class="bi bi-person-circle"></a></li><br>
                        <%}%>
                   
                </ul>
                
                
              
              
            </header>
   <div class="top"></div>
   <div class="container">
    <div class="row pt-5">
      <% for(let i = 0; i < datos.length; i++) { %>
        <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4">
          <div class="card h-100">
            <a href="details?id=<%= datos[i].id %>">
              <img src="<%= urls[i] %>" class="card-img-top" alt="<%= datos[i].img_product %>">
            </a>
            <div class="card-body text-center">
              <h5 class="card-title"><%= datos[i].nombre_prod %></h5>
              <p class="card-text text-muted"><b>$<%= parseFloat(datos[i].precio_publico).toLocaleString() %>.00</b></p>
            </div>
          </div>
        </div>
      <% } %>
    </div>
  </div>
  <script src="script/responsive.js"></script>
   
        
    
    